/* miniprogram/pages/denglu/denglu.wxss */
page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
}
.container{
  margin-top: 50rpx;
  padding: 0;
  width: 100%;
}
.title{
  font-size: 28rpx;
  color:#7c7e7e;
}
.block{
  margin: 20rpx 0;
}
input,.unit{
  text-align: left;
  height:60rpx;
  width: 600rpx;
  display: flex;
  justify-items: center;
  background: #f0f0f0;
}
.unit .text{
  margin: 10rpx 0;
}
.bottom{
  width: 100%;
  height: 200rpx;
  font-size: 25rpx;
  margin-top: 50rpx;
  color: blue;
  justify-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
}
.bottom text{
  margin: 50rpx 0 ;
}


